<div class="dropdown">
  <button class="btn btn-primary dropdown-toggle" type="button" id="create-dropdown-button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    <i class="fa fa-plus"></i> {{_ 'label.create'}} {{_ labelKey 1}}
  </button>
  <div class="dropdown-menu dropdown-menu-right" aria-labelledby="create-dropdown-button">
    {{#each objectGroup}}
      <a class="dropdown-item" href="{{url ../route typeId}}">{{_ label}}</a>
    {{/each}}
  </div>
</div>